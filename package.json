{
  "name": "ferryjoy-client",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "bun x expo start",
    "web": "clear && expo start -c --web",
    "android": "expo start -c --android",
    "ios": "expo start -c --ios",
    "clean": "rm -rf .expo node_modules",
    "postinstall": "bun x tailwindcss -i ./src/global.css -o ./node_modules/.cache/nativewind/global.css",
    "prepare": "bun run postinstall",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "format": "biome format src/",
    "format:fix": "biome format --write src/",
    "check": "biome check src/",
    "check:fix": "biome check --write --unsafe src/",
    "organize-imports": "biome check --write --unsafe src/",
    "typecheck": "tsc --noEmit",
    "check:all": "bun run lint && bun run format && bun run typecheck",
    "fix:all": "bun run check:fix && bun run organize-imports",
    "reset": "bun run clean && bun install",
    "optimize-routes": "node scripts/optimize-routes.js",
    "convex:dev": "bun x convex dev",
    "convex:deploy": "bun x convex deploy",
    "convex:deploy:prod": "bun x convex deploy --prod",
    "convex:codegen": "bun x convex codegen",
    "convex:typecheck": "bun x convex typecheck",
    "convex:run": "bun x convex run",
    "convex:run:prod": "bun x convex run --prod",
    "convex:logs": "bun x convex logs",
    "convex:logs:prod": "bun x convex logs --prod",
    "convex:dashboard": "bun x convex dashboard",
    "convex:auth": "bun x convex auth",
    "convex:init": "bun x convex init",
    "convex:dev:all": "bun run convex:dev & bun run start"
  },
  "dependencies": {
    "@convex-dev/crons": "^0.1.9",
    "@gorhom/bottom-sheet": "^5.1.8",
    "@math.gl/web-mercator": "^4.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.10",
    "@react-navigation/stack": "^7.4.5",
    "@rn-primitives/avatar": "~1.2.0",
    "@rn-primitives/dropdown-menu": "^1.2.0",
    "@rn-primitives/portal": "~1.3.0",
    "@rn-primitives/progress": "~1.2.0",
    "@rn-primitives/slot": "^1.2.0",
    "@rn-primitives/tooltip": "~1.2.0",
    "@rnmapbox/maps": "^10.1.40",
    "@tanstack/query-persist-client-core": "^5.83.1",
    "@tanstack/query-sync-storage-persister": "^5.83.1",
    "@tanstack/react-query": "^5.84.2",
    "@turf/turf": "^7.2.0",
    "@types/three": "^0.178.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.25.4",
    "expo": "^53.0.20",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-linking": "~7.1.7",
    "expo-location": "^18.1.6",
    "expo-navigation-bar": "~4.2.7",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "lucide-react-native": "^0.511.0",
    "mapbox-gl": "^3.14.0",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-map-gl": "^8.0.4",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-logs": "^5.3.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "~0.20.0",
    "react-native-worklets": "^0.4.1",
    "tailwind-merge": "^2.6.0",
    "three": "^0.178.0",
    "viewport-mercator-project": "^7.0.4",
    "ws-dottie": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@biomejs/biome": "^2.1.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/node": "^24.2.1",
    "@types/react": "~19.0.14",
    "babel-plugin-react-compiler": "19.1.0-rc.2",
    "expo-dev-client": "~5.2.4",
    "jsdom": "^25.0.1",
    "tailwindcss": "3.3.5",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3"
  },
  "private": true
}
