{
  "name": "ferryjoy-client",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "bun x expo start",
    "web": "clear && expo start -c --web",
    "android": "expo start -c --android",
    "ios": "expo start -c --ios",
    "clean": "rm -rf .expo node_modules",
    "postinstall": "bun x tailwindcss -i ./src/global.css -o ./node_modules/.cache/nativewind/global.css",
    "prepare": "bun run postinstall",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "format": "biome format src/",
    "format:fix": "biome format --write src/",
    "check": "biome check src/",
    "check:fix": "biome check --write --unsafe src/",
    "organize-imports": "biome check --write --unsafe src/",
    "typecheck": "tsc --noEmit",
    "check:all": "bun run lint && bun run format && bun run typecheck",
    "fix:all": "bun run check:fix && bun run organize-imports",
    "reset": "bun run clean && bun install",
    "supabase:types": "bunx supabase gen types typescript --project-id 'szwidfuttfsfauyjpfkr' --schema public > src/data/supabase/database.types.ts",
    "optimize-routes": "node scripts/optimize-routes.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/native": "^7.1.6",
    "@rn-primitives/avatar": "~1.2.0",
    "@rn-primitives/dropdown-menu": "^1.2.0",
    "@rn-primitives/portal": "~1.3.0",
    "@rn-primitives/progress": "~1.2.0",
    "@rn-primitives/slot": "^1.2.0",
    "@rn-primitives/tooltip": "~1.2.0",
    "@rnmapbox/maps": "^10.1.39",
    "@supabase/supabase-js": "^2.52.1",
    "@tanstack/query-persist-client-core": "^5.83.0",
    "@tanstack/query-sync-storage-persister": "^5.83.0",
    "@tanstack/react-query": "^5.83.0",
    "@turf/turf": "^7.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo": "^53.0.20",
    "expo-constants": "~17.1.4",
    "expo-linking": "~7.1.4",
    "expo-router": "~5.1.4",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.6",
    "expo-web-browser": "~14.2.0",
    "expo-location": "^18.1.6",
    "expo-navigation-bar": "~4.2.7",
    "expo-splash-screen": "~0.30.10",
    "lucide-react-native": "^0.511.0",
    "mapbox-gl": "^3.13.0",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-map-gl": "^8.0.4",
    "react-native": "0.79.5",
    "react-native-logs": "^5.3.0",
    "react-native-reanimated": "~3.17.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "~0.20.0",
    "supabase": "^2.31.8",
    "tailwind-merge": "^2.6.0",
    "ws-dottie": "^0.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@biomejs/biome": "^2.1.2",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/node": "^24.1.0",
    "@types/react": "~19.0.14",
    "expo-dev-client": "~5.2.4",
    "jsdom": "^25.0.1",
    "tailwindcss": "3.3.5",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "private": true
}
